/* pages/lanuch/lanuch.wxss */

.launch-bg {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.launch-bg-image {
  width: 100%;
  height: 100%;
  position: absolute;
}

.launch-enter {
  margin-bottom: 350rpx;
  border: 1px solid #fff;
  width: 200rpx;
  height: 80rpx;
  border-radius: 5px;
  text-align: center;
  bottom: 0;
  position: absolute;
}

.launch-enter-text {
  font-size: 24rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #fff;
}
